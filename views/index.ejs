<!doctype html>
<html>
<head>
  <title>Answer for: Expressjs + socket.io + express-session</title>
</head>
<body>
  <p>AJDUPAAX example</p>
  <button id="btn">Click me</button>
  <script src="https://code.jquery.com/jquery-2.1.3.js"></script>
  <script>
    $(function() {

      $('#btn').click(function () {
        console.log('Rozpoczęto zapytanie do serwera.');

        $.ajax({
          method: 'GET',
          url: '/data.json',
          success: function(data) {
            console.log('Otrzymano odpowiedź z serwera.', data);
          }
        });
      });
    });
  </script>
</body>
</html>